    function Parça_Genel_Kar_Oranı_Hesapla(İşçilik_Maliyeti_Karsız_Toplam, Malzeme_Maliyeti_Karsız_Toplam, Parça_Toplam_Karlı_Maliyet) {
        var Toplam_Karsız_Maliyet = parseFloat(İşçilik_Maliyeti_Karsız_Toplam) + parseFloat(Malzeme_Maliyeti_Karsız_Toplam)
        var a = parseFloat(Toplam_Karsız_Maliyet);
        var b = parseFloat(Parça_Toplam_Karlı_Maliyet);
        var yüzde_Genel = ((b - a) / a) * parseFloat(100);

        return yüzde_Genel;

    }
