            return parseFloat(İşçilik_Maliyeti) + (parseFloat(İşçilik_Maliyeti) * ((parseFloat(Kar_Oranı)) / parseFloat(100)))
        }
        catch (err) {
            return null
        }

    }

    function Malzeme_Hurda_Fiyatı_Hesapla(Malzeme_Maliyeti, Malzeme_Birim_Fiyatı, Hurda_Birim_Satış_Oranı) {

        try {
            return (parseFloat(Malzeme_Birim_Fiyatı) / parseFloat(100)) * parseFloat(Hurda_Birim_Satış_Oranı)
        }
        catch (Err) {
            return null
        }


    }

    function Malzeme_Fire_Maliyeti_Hesapla(Malzeme_Maliyeti, Hurda_Birim_Satış_Oranı, Malzeme_Fire_Oranı) {

        try {

            var Malzeme_Fireli_Maliyeti = parseFloat(Malzeme_Maliyeti) * (parseFloat(Malzeme_Fire_Oranı) / parseFloat(100))

            var Fire_Maliyeti = parseFloat(Malzeme_Fireli_Maliyeti) * (parseFloat(Hurda_Birim_Satış_Oranı) / (parseFloat(100)))


            return Fire_Maliyeti
        }
        catch (err) {
            return null
        }




    }
