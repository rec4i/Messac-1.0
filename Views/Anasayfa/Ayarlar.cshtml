        function DısOperasyon_Build_Table($el, cells, rows) {
            
            $el.bootstrapTable('destroy');
           

            var i; var j; var row
            var columns = [];
            var data__ = [];
            columns.push({
                field: 'ID',
                title: 'ID',
                sortable: true
            })
            columns.push({
                field: 'Operasyon_Adı',
                title: 'Operasyon Adı',
                sortable: true
            })
            columns.push({
                field: 'Birim',
                title: 'Birim',
                sortable: true
            })
          
            columns.push({
                field: 'Birim_Maliyet',
                title: 'Birim Maliyet',
                sortable: true
            })
            columns.push({
                field: 'Düzenle',
                title: 'Düzenle',
            })
            columns.push({
                field: 'Sil',
                title: 'Sil',
            })
            
            
            $.ajax({
                url: 'api/DısOperasyon/DısOperasyon_Get_All',
                type: 'POST',
                async: false,
                dataType: "json",
                contentType: "application/json; charset=utf-8",
                success: function (data) {

                    var temp = data
                    for (i = 0; i < temp.length; i++) {
                        row = {}
                        var Fiyatı = '' + 
                        '<input   id="Operasyon_Maliyet_İnput_Mask" class="form-control" data-thousands="." data-decimal=","' + 
                        '                                           value="'+temp[i].operasyon_Maliyeti.toFixed(2)+'"         data-suffix=" ₺" type="text"' + 
                        '                                                      />' + 
                        '';
                       
                        
            

                        row['ID'] = temp[i].id
                        row['Operasyon_Adı'] = temp[i].operasyon_Adı
                        row['Birim'] = temp[i].birim.birim_Kısa_ad
                        row['Birim_Maliyet'] = Fiyatı
                        row['Sil'] = '<button class="btn btn-danger"  Operasyon_ıd="'+temp[i].id+'"  id="Dıs_Operasyon_Sil"><i class="fa fa-trash"></i></button>'
                        row['Düzenle'] = '<button class="btn btn-block btn-warning" Operasyon_ıd="'+data[i].id+'"  id="Dıs_Operasyon_Düzenle">Düzenle</button>'  

                        data__.push(row)


                    }
                },
                error: function () {
                    alert('Talep esnasında sorun oluştu.Yeniden deneyin');
                }
            });



