                                    Swal.fire({
                                        icon: 'error',
                                        title: 'Talep Esnasında Bir Hata Oluştur',
                                        text: '' + xhr.status + '   ' + error.Message,
                                    })
                                }

                            }
                        })

                        
                    }
                })


            })

            $('button[id=Dıs_Operasyon_Düzenle]').click(function(){
